@page
@inherits EasyAbp.UniappManagement.Web.Pages.UniappManagementPage
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model EasyAbp.UniappManagement.Web.Pages.Uniapps.Uniapp.CreateModalModel
@{
    Layout = null;
}
<abp-dynamic-form abp-model="Uniapp" data-ajaxForm="true" asp-page="/Uniapps/Uniapp/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["CreateUniapp"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>