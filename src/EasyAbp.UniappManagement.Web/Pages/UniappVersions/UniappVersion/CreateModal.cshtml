@page
@inherits EasyAbp.UniappManagement.Web.Pages.UniappManagementPage
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model EasyAbp.UniappManagement.Web.Pages.UniappVersions.UniappVersion.CreateModalModel
@{
    Layout = null;
}
<abp-dynamic-form abp-model="UniappVersion" data-ajaxForm="true" asp-page="/UniappVersions/UniappVersion/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["CreateUniappVersion"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content/>
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>